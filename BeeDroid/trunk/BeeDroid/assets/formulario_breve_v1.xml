<?xml version='1.0' encoding='UTF-8'?><!-- <!DOCTYPE form SYSTEM 'form_def.dtd'> -->
<form>
	<meta>
		<id />
		<version>1</version>
		<name>Formulario de prueba</name>
		<author>
			<user>turawet</user>
		</author>
		<geolocalized />
	</meta>
	<sections>
		<section>
			<id />
			<name>Primera sección</name>
			<fields>
				<field>
					<id />
					<label>Radio1</label>
					<type>RADIO</type>
					<required />
					<options>
						<option>
							<label>Rojo</label>
							<value>R</value>
						</option>
						<option>
							<label>Azul</label>
							<value>A</value>
						</option>
						<option>
							<label>Verde</label>
							<value>V</value>
						</option>
						<option>
							<label>Marrón</label>
							<value>M</value>
						</option>
						<option>
							<label>Negro</label>
							<value>N</value>
						</option>
					</options>
				</field>
				<field>
					<id />
					<label>Etiqueta2</label>
					<type>TEXT</type>
					<properties>
						<property>
							<name>font</name>
							<value>Arial</value>
						</property>
					</properties>
				</field>
				<field>
					<id />
					<label>Fecha1</label>
					<type>DATE</type>
					<required />
					<properties>
						<property>
							<name>format</name>
							<value>YYYY/MM/DD</value>
						</property>
						<property>
							<name>calendar</name>
							<value>MONTH_CALENDAR</value>
						</property>
					</properties>
				</field>

			</fields>
		</section>
	</sections>
</form>