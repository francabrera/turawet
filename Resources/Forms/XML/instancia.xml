<!DOCTYPE form SYSTEM "instance_def.dtd">

<form>
	<id>1</id>>
	<name>Formulario de prueba</name>
	<version>1</version>
	<meta>
		<author>
			<user>turawet</user>
		</author>
		<creationdate>12/01/11</creationdate>
		<modificationdate>12/01/11</modificationdate>
	</meta>
	<sections>
		<section>
			<name>Primera sección</name>
			<instances>
				<instancefields>
					<id />
					<fieldid>1</fieldid>
					<label>Etiqueta1</label>
					<type>TEXT</type>
					<required />
					<value>Texto escrito</value>
				</instancefields>
				<instancefields>
					<id />
					<fieldid>1</fieldid>
					<label>Etiqueta2</label>
					<type>TEXT</type>
					<properties>
						<property>
							<name>font</name>
							<value>Arial</value>
						</property>
					</properties>
				</instancefields>
				<instancefields>
					<id />
					<fieldid>1</fieldid>
					<label>Fecha1</label>
					<type>DATE</type>
					<required />
					<properties>
						<property>
							<name>format</name>
							<value>YYYY/MM/DD</value>
						</property>
						<property>
							<name>calendar</name>
							<value>MONTH_CALENDAR</value>
						</property>
					</properties>
				</instancefields>
				<instancefields>
					<id />
					<fieldid>1</fieldid>
					<label>Radio1</label>
					<type>RADIO</type>
					<required />
					<options>
						<option>
							<label>Masculino</label>
							<value>M</value>
						</option>
						<option>
							<label>Femenino</label>
							<value>F</value>
						</option>
					</options>
				</instancefields>
				<instancefields>
					<id />
					<fieldid>1</fieldid>
					<label>Combo1</label>
					<type>COMBO</type>
					<options>
						<option>
							<label>Opción 1</label>
							<value>O1</value>
							<default />
						</option>
						<option>
							<label>Opción 2</label>
							<value>O2</value>
						</option>
					</options>
					<properties>
						<property>
							<name>size</name>
							<value>30</value>
						</property>
					</properties>
				</instancefields>
				<instancefields>
					<id />
					<fieldid>1</fieldid>
					<label>Check1</label>
					<type>CHECKBOX</type>
					<required />
					<properties>
						<property>
							<name>widget</name>
							<value>TRISTATE</value>
						</property>
					</properties>
				</instancefields>
				<instancefields>
					<id />
					<fieldid>1</fieldid>
					<label>Imágenes</label>
					<type>IMAGE_GALLERY</type>
					<required />
					<properties>
						<property>
							<name>max_photos</name>
							<value>6</value>
						</property>
					</properties>
				</instancefields>
				<instancefields>
					<id />
					<fieldid>1</fieldid>
					<label>Vídeos</label>
					<type>VIDEO_GALLERY</type>
					<properties>
						<property>
							<name>max_file_size</name>
							<value>128</value>
						</property>
					</properties>
				</instancefields>
				<instancefields>
					<id />
					<fieldid>1</fieldid>
					<label>Audio</label>
					<type>AUDIO_GALLERY</type>
					<properties>
						<property>
							<name>max_file_size</name>
							<value>8</value>
						</property>
					</properties>
				</instancefields>
				<instancefields>
					<id />
					<fieldid>1</fieldid>
					<label>Localización</label>
					<type>LOCATION</type>
					<position>
						<latitude>23.516</latitude>
						<longitude>-49.21</longitude>
					</position>
					<properties>
						<property>
							<name>type</name>
							<value>COORDINATES</value>
						</property>
					</properties>
				</instancefields>
				<!--  Grupo sin lista, con un idgroup que no se repite -->
				<instancegroup>
					<idgroup>0</idgroup>
					<instancefields>
						<id />
						<fieldid>1</fieldid>
						<label>Localización</label>
						<type>LOCATION</type>
						<position>
							<latitude>23.516</latitude>
							<longitude>-49.21</longitude>
						</position>
						<properties>
							<property>
								<name>type</name>
								<value>COORDINATES</value>
							</property>
						</properties>
					</instancefields>
				</instancegroup>
				<!--
					A continuación dos elementos de una lista con un mismo idgroup y
					mismo fielid
				-->
				<instancegroup>
					<idgroup>1</idgroup> <!-- Primer elemento de la lista -->
					<instancefields>
						<id />
						<fieldid>1</fieldid>
						<label>Nombre</label>
						<type>TEXT</type>
						<required />
						<value>Texto escrito</value>
					</instancefields>
					<instancefields>
						<id />
						<fieldid>2</fieldid>
						<label>Apellido</label>
						<type>TEXT</type>
						<required />
						<value>Texto escrito</value>
					</instancefields>
				</instancegroup>
				<instancegroup>
					<idgroup>1</idgroup> <!-- Segundo elemento de la lista -->
					<instancefields>
						<id />
						<fieldid>1</fieldid>
						<label>Nombre</label>
						<type>TEXT</type>
						<required />
						<value>Texto escrito2</value>
					</instancefields>
					<instancefields>
						<id />
						<fieldid>2</fieldid>
						<label>Apellido</label>
						<type>TEXT</type>
						<required />
						<value>Texto escrito2</value>
					</instancefields>
				</instancegroup>
			</instances>
		</section>
	</sections>
</form>

